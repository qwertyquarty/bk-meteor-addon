{
  "schemaVersion": 1,
  "id": "bk-meteor-addon",
  "version": "${version}",
  "name": "Bk Meteor Addon",
  "authors": [
    "bknibb"
  ],
  "contact": {
    "repo": "https://github.com/Bknibb/bk-meteor-addon"
  },
  "license": "GPL-3.0",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "org.bknibb.bk_meteor_addon.BkMeteorAddon"
    ]
  },
  "mixins": [
    "bk-meteor-addon.mixins.json",
    "bk-meteor-addon.meteor-rejects.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "25,255,255",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=21",
    "minecraft": "*",
    "meteor-client": "*"
  }
}
